<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jw.jwvisit.dao.VisitDao" >
  
  <insert id="createvisit" parameterType="com.jw.jwvisit.model.visitinfo" >
    insert into visitinfo (visitedname,visitedpnum,visitedevent,visittime,lefttime,visitorname,identitynum,platenum,visitorpnum,imgurl)
    values (#{visitedname},#{visitedpnum},#{visitedevent},#{visittime},#{lefttime},#{visitorname},#{identitynum},#{platenum},#{visitorpnum},#{imgurl})
  </insert>
  
</mapper>